{{#list}}
<li class="p-item">
    <div class="p-img-con">
        <a href="./detail.html?productId={{id}}" class="link" target="_blank">
            <img class="p-img" alt="{{name}}" src="{{imageHost}}{{mainImage}}" />
        </a>
    </div>
    <div class="p-price-con">
        <span class="p-price">ï¿¥{{price}}</span>
    </div>
    <div class="p-name-con">
        <a href="./detail.html?productId={{id}}" class="p-name" target="_blank">
            {{name}}
        </a>
    </div>
</li>
{{/list}}